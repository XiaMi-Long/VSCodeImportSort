{
  "name": "import-sort",
  "displayName": "ES6 Import Sort",
  "description": "对 ES6 的 import 行进行字符长度排序",
  "version": "0.0.1",
  "icon": "./icon/logo.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XiaMi-Long/VSCodeImportSort"
  },
  "publisher": "wwy",
  "categories": [
    "Other"
  ],
  "keywords": [
    "ES6 Import Sort",
    "ES6",
    "Import",
    "Sort",
    "String Sort"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "import-sort.desc-sort-import",
        "title": "import-sort(降序)"
      },
      {
        "command": "import-sort.asc-sort-import",
        "title": "import-sort(升序)"
      },
      {
        "command": "import-sort.desc-sort-common",
        "title": "common-sort(降序)"
      },
      {
        "command": "import-sort.asc-sort-common",
        "title": "common-sort(升序)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}